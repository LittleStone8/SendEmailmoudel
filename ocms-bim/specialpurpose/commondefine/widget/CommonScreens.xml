    <screen name="main-decorator">
        <section>
            <actions>
            	<service service-name="getHomeApplications" result-map="result"></service>
            	<set field="imei" from-field="result.imei"/>
                <set field="apps" from-field="result.apps"/>
                <set field="currenttime" from-field="result.currenttime"/>
                <set field="timeZoneId" from-field="result.timeZoneId"/>
                <set field="rawOffset" from-field="result.rawOffset"/>
                <set field="timeNow" from-field="result.timeNow"/>
                <set field="isShip" from-field="result.isShip"/>
                <set field="SystemSettingsUomid" from-field="result.SystemSettingsUomid"/>
                <set field="piwikHost" from-field="result.piwikHost"/>
                <set field="piwikPort" from-field="result.piwikPort"/>
                <set field="piwikSiteId" from-field="result.piwikSiteId"/>
            </actions>
            <widgets>
                 <platform-specific>
	                 <html>
	                	 <html-template location="component://commondefine/includes/header.ftl"/>
	                 </html>
                 </platform-specific>
                 <decorator-section-include name="body"/>
                 <platform-specific>
                  <html>
                 		 <html-template location="component://commondefine/includes/footer.ftl"/>
                  </html>
                 </platform-specific>
            </widgets>
        </section>
    </screen>
    
